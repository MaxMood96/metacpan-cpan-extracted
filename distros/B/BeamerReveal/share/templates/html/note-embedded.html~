<img id="---NOTEID---"
     width="100%" height="100%">
<script type="module">
  const encoded = `
---NOTEEMBEDDEDB64---`;
  const bytes = Uint8Array.from(atob(encoded), c =>c.charCodeAt(0));
  const blob = new Blob([bytes], { type: "---MIMETYPE---" });
    
  const note = document.getElementById("---NOTEID---");
  note.src = URL.createObjectURL(blob);
</script>
