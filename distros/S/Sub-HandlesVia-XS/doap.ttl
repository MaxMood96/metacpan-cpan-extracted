<http://purl.org/NET/cpan-uri/dist/Sub-HandlesVia-XS/v_0-001001>
	a                    doap:Version;
	dc:identifier        "Sub-HandlesVia-XS-0.001001"^^xsd:string;
	dc:issued            "2026-01-28"^^xsd:date;
	doap-changeset:changeset [
		doap-changeset:item [
			a doap-changeset:Bugfix;
			rdfs:label "Fix segfaults in older versions of Perl caused by trying to POPs without checking the function we called returned a value at all. Now always check a value was returned first!";
		], [
			a doap-changeset:Change;
			rdfs:label "Defer to Type::Tiny's error handling when a type check fails, so that error messages are at least approximately the same.";
		], [
			a doap-changeset:Change;
			rdfs:label "Change missing parameter error messages to 'Wrong number of parameters' for closer consistency with Type::Params.";
		], [
			a doap-changeset:Bugfix;
			rdfs:label "Fix for hard to trigger refcount issues with shift and pop methods.";
		], [
			a doap-changeset:Change;
			rdfs:label "Issue 'Wrong number of parameters' errors when too many parameters are given.";
		], [
			a doap-changeset:Change;
			rdfs:label "More consistency in whether the invocant is counted as a parameter (it always is now) when reporting which parameter number had an issue.";
		], [
			a doap-changeset:Addition;
			rdfs:label "Internal structures which have an 'element_type' now also can have an 'element_type_tiny' which is a pointer back to the original Type::Tiny type. If this is provided, then error handling should be even better.";
		], [
			a doap-changeset:Change;
			rdfs:label "The push and unshift methods now return the new array length.";
		], [
			a doap-changeset:Change;
			rdfs:label "Check strings passed to join methods are actually strings.";
		], [
			a doap-changeset:Change;
			rdfs:label "Check parameters passed to sort methods are actually coderefs.";
		], [
			a doap-changeset:Change;
			rdfs:label "Check indices passed to get, set, and accessor methods are actually integers.";
		];
	];
	doap-changeset:released-by <http://purl.org/NET/cpan-uri/person/tobyink>;
	doap:file-release    <http://backpan.cpan.org/authors/id/T/TO/TOBYINK/Sub-HandlesVia-XS-0.001001.tar.gz>;
	doap:revision        "0.001001"^^xsd:string.

